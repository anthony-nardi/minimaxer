{
  "name": "minimax-ts",
  "version": "1.0.0",
  "description": "Library for building game trees and running the minimax algorithm for turn-based games",
  "keywords": [
    "minimax",
    "game"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/domw95/minimax-ts.git"
  },
  "author": "domw95",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/domw95/minimax-ts/issues"
  },
  "homepage": "https://github.com/domw95/minimax-ts#readme",
  "devDependencies": {
    "prettier": "^2.8.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.4"
  },
  "files": [
    "lib/**/*"
  ]
}
